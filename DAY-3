#include <stdio.h>

int main()
{
    int n, key, i;
    int arr[100];
    int comparisons = 0;
    int found = 0;

    scanf("%d", &n);

    for (i = 0; i < n; i++)
        scanf("%d", &arr[i]);

    scanf("%d", &key);

    for (i = 0; i < n; i++)
    {
        comparisons++;
        if (arr[i] == key)
        {
            found = 1;
            break;
        }
    }

    if (found)
        printf("Element found at index %d\n", i);
    else
        printf("Element not found\n");

    printf("Total comparisons = %d\n", comparisons);

    return 0;
}

